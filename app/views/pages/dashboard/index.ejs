<h2>authors</h2>

<ul>
  <% authors.forEach(author=> { %>

    <li>
      <a href="/authors/<%= author._id %>">
        <%= author.listname %>
      </a>
    </li>

    <% }) %>
</ul>


<h2>books</h2>

<ul>
  <% books.forEach(book=> { %>

    <li>
      <a href="/books/<%= book._id %>">
        <%= book.title %>
      </a>
      by
      <a href="/authors/<%= book.author._id %>">
        <%= book.author.fullname %>
      </a>
    </li>

    <% }) %>
</ul>