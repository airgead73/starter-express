<article>
  <h2>
    <%= typeof heading !='undefined' ? heading : 'heading' %>
  </h2>
  <p><a href="/authors/drop">drop</a> author collection</p>

  <ul>
    <% authors.forEach(author=> { %>

      <li>
        <a href="/authors/<%= author._id %>">
          <%= author.listname %>
        </a>
      </li>

      <% }) %>
  </ul>
</article>